@code {
    [CascadingParameter]
    private HttpContext HttpContext { get; set; } 

    private IComponentRenderMode RenderModeForPage => HttpContext.Request.Path.StartsWithSegments("/Account")
        ? null
        : new InteractiveServerRenderMode(prerender: false);
}