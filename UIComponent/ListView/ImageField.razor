@namespace Caspian.UI
@typeparam TEntity
@{
    if (RowData == null) 
        (DataView as IListViewer<TEntity>).AddDataField(Field.Body);
}
@if (RowData != null)
{
    if (Template == null)
    {
        <PictureBox Data="Field.Compile().Invoke(RowData.Data)" style="@Style" />
    }
    else
    {
        @Template
    }
}