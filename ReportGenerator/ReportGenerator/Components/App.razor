    <script type="text/javascript">
        function getPixelsPerCentimetre() {
            let div = document.createElement("div");
            div.style.width = "1cm";
            document.body.appendChild(div);
            let result = div.getBoundingClientRect().width;
            document.body.removeChild(div);
            return result;
        }
    </script>
