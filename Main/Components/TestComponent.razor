@page "/Test1"
@using Caspian.UI
@using Demo.Model
@using Demo.Service
@using Microsoft.EntityFrameworkCore
@inherits BasePage


@code 
{
    Stimulsoft.Report.StiReport report;
    StringTextBox txt;
    bool flag = true;

    async void Focus()
    {
        await txt.FocusAsync();
    }

    protected override async Task OnInitializedAsync()
    {
        report = new Stimulsoft.Report.StiReport();
        using var service = CreateScope().GetService<OrderDeatilService>();
        var list = await service.GetAll().ToListAsync();
        report.RegBusinessObject("test", list);
        await base.OnInitializedAsync();
    }
}
